<% if(user.role === "teacher"){ %>
 <%-include("teacher/teacher-dash.ejs")%>
<% }else{ %>
  <%-include("student/student-dash.ejs")%>
<% } %>
<section class="upload-course">
   <div class="container">
    <form action="/update/<%= user._id %>" method="POST" enctype="multipart/form-data">
    <div class="form-group">
        <label for="name">New Name</label><br>
        <input type="text" class="form-control" autofocus required name="name" id="name" placeholder="Enter New Name"><br>
    </div>
    <div class="form-group">
        <label for="email-add">New Email Address</label><br>
        <input type="email" class="form-control" name="email" required id="email-add" placeholder="Enter New Email Address"><br>
    </div>

    <button type="submit" class="btn btn-secondary">Submit</button>
    </form>
   </div>
</section>
<%-include("partials/dash-footer.ejs")%>